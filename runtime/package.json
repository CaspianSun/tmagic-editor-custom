{
  "name": "drag-code-runtime",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build:upload": "npm run build && sh ../upload.sh",
    "build": "rm -rf dist && run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "build:libs": "tmagic entry && npm run build:config && npm run build:value && npm run build:event && npm run build:ds:libs",
    "build:ds:libs": "npm run build:ds:config && npm run build:ds:value && npm run build:ds:event",
    "build:config": "vite build --config entry.vite.config.ts --mode config --emptyOutDir",
    "build:value": "vite build --config entry.vite.config.ts --mode value --emptyOutDir",
    "build:event": "vite build --config entry.vite.config.ts --mode event --emptyOutDir",
    "build:ds:config": "vite build --config entry.vite.config.ts --mode ds:config --emptyOutDir",
    "build:ds:value": "vite build --config entry.vite.config.ts --mode ds:value --emptyOutDir",
    "build:ds:event": "vite build --config entry.vite.config.ts --mode ds:event --emptyOutDir"
  },
  "dependencies": {
    "@vant/area-data": "^1.5.1",
    "animejs": "^3.2.2",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "qrcode": "^1.5.3",
    "scrollreveal": "^4.0.9",
    "swiper": "^11.0.7",
    "vant": "^4.8.5",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.2",
    "@tmagic/cli": "^1.3.15",
    "@tmagic/core": "^1.3.15",
    "@tmagic/data-source": "^1.3.15",
    "@tmagic/schema": "^1.3.15",
    "@tmagic/stage": "^1.3.15",
    "@tmagic/utils": "^1.3.15",
    "@tsconfig/node18": "^18.2.2",
    "@types/animejs": "^3.1.12",
    "@types/jsdom": "^21.1.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.18.8",
    "@types/qrcode": "^1.5.5",
    "@types/scrollreveal": "^0.0.11",
    "@vant/use": "^1.6.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.2",
    "pont-engine": "^1.6.2",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.71.1",
    "typescript": "^5.3.3",
    "unocss": "^0.58.5",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "vue-tsc": "^2.0.6"
  }
}
