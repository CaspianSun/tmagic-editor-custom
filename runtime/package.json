{
  "name": "drag-code-runtime",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "build:libs": "tmagic entry && npm run build:config && npm run build:value && npm run build:event && npm run build:ds:libs",
    "build:ds:libs": "npm run build:ds:config && npm run build:ds:value && npm run build:ds:event",
    "build:config": "vite build --config entry.vite.config.ts --mode config --emptyOutDir",
    "build:value": "vite build --config entry.vite.config.ts --mode value --emptyOutDir",
    "build:event": "vite build --config entry.vite.config.ts --mode event --emptyOutDir",
    "build:ds:config": "vite build --config entry.vite.config.ts --mode ds:config --emptyOutDir",
    "build:ds:value": "vite build --config entry.vite.config.ts --mode ds:value --emptyOutDir",
    "build:ds:event": "vite build --config entry.vite.config.ts --mode ds:event --emptyOutDir"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "axios": "^1.6.1",
    "lodash": "^4.17.21",
    "qrcode": "^1.5.3",
    "swiper": "^11.0.4",
    "vant": "^4.7.3",
    "vue": "^3.3.8",
    "wow.js": "^1.2.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@tmagic/cli": "../../tmagic-editor/packages/cli",
    "@tmagic/core": "../../tmagic-editor/packages/core",
    "@tmagic/data-source": "../../tmagic-editor/packages/data-source",
    "@tmagic/schema": "../../tmagic-editor/packages/schema",
    "@tmagic/stage": "../../tmagic-editor/packages/stage",
    "@tmagic/utils": "../../tmagic-editor/packages/utils",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.5",
    "@types/lodash": "^4.14.201",
    "@types/node": "^18.18.8",
    "@types/qrcode": "^1.5.5",
    "@unocss/transformer-variant-group": "^0.57.3",
    "@vant/use": "^1.6.0",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.53.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "scss": "^0.2.4",
    "typescript": "~5.2.2",
    "unocss": "^0.57.3",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  }
}
