{
  "name": "drag-code-runtime",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "entry": "tmagic entry",
    "build": "vite build",
    "build:libs": "tmagic entry && npm run build:config && npm run build:value && npm run build:event && npm run build:ds:libs",
    "build:ds:libs": "npm run build:ds:config && npm run build:ds:value && npm run build:ds:event",
    "build:config": "vite build --config entry.vite.config.ts --mode config",
    "build:value": "vite build --config entry.vite.config.ts --mode value",
    "build:event": "vite build --config entry.vite.config.ts --mode event",
    "build:ds:config": "vite build --config entry.vite.config.ts --mode ds:config",
    "build:ds:value": "vite build --config entry.vite.config.ts --mode ds:value",
    "build:ds:event": "vite build --config entry.vite.config.ts --mode ds:event"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "lodash": "^4.17.21",
    "vue": "^3.3.7",
    "wow.js": "^1.2.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@tmagic/cli": "1.3.0-beta.8",
    "@tmagic/core": "1.3.0-beta.8",
    "@tmagic/data-source": "1.3.0-beta.8",
    "@tmagic/schema": "1.3.0-beta.8",
    "@tmagic/stage": "1.3.0-beta.8",
    "@tmagic/utils": "1.3.0-beta.8",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.4",
    "@types/lodash": "^4.14.200",
    "@types/node": "^18.18.8",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.53.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "scss": "^0.2.4",
    "typescript": "~5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  }
}
